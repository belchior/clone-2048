{
  "name": "clone-2048",
  "version": "2.0.5",
  "description": "This is a fun and shameless copy of the game 2048 made by Gabriele Cirulli",
  "author": "Belchior Oliveira (https://github.com/belchior)",
  "homepage": "https://belchior.github.io/clone-2048",
  "repository": {
    "type": "git",
    "url": "git@github.com:belchior/clone-2048.git"
  },
  "license": "MIT",
  "keywords": [
    "game",
    "2048",
    "for fun"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "lint": "npx eslint ./src/"
  },
  "dependencies": {
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  }
}
