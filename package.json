{
  "name": "clone-2048",
  "version": "2.1.8",
  "description": "This is a fun and shameless copy of the game 2048 made by Gabriele Cirulli",
  "author": "Belchior Oliveira (https://github.com/belchior)",
  "homepage": "https://belchior.github.io/clone-2048",
  "repository": {
    "type": "git",
    "url": "git@github.com:belchior/clone-2048.git"
  },
  "license": "MIT",
  "keywords": [
    "game",
    "2048",
    "for fun"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "dependencies": {
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.2",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.0.6",
    "cross-env": "^6.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.4.0",
    "eslint-plugin-jest": "^22.17.0",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "@types/jest": "^24.0.18"
  }
}
